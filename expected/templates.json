{
  "environment": "production",
  "region": "us-east-1",
  "url": "production-us-east-1",
  "path": "/var/log/production",
  "Server": [
    {
      "name": "web1",
      "env": "production",
      "log_path": "/logs/production/web1",
      "description": "Server web1 in production"
    },
    {
      "name": "db1",
      "env": "staging",
      "log_path": "/logs/staging/db1",
      "description": "Database db1"
    }
  ],
  "Config": [
    {
      "key": "region_key",
      "value": "region",
      "message": "Region is region"
    },
    {
      "key": "env_key",
      "value": "environment",
      "message": "Environment: environment"
    }
  ]
}
